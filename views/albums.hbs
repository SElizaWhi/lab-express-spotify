<link rel="stylesheet" href="/styles/style.css">
<h1>Albums</h1>
<div class="all-results">
{{#each albumsData.body.items}}
    <section class="card">
        <h2>Name: {{name}}</h2>
        <img src="{{images.0.url}}" alt="" srcset="">
        <a href="/tracks/{{id}}">View Tracks</a>
    </section>
{{/each}}
</div>